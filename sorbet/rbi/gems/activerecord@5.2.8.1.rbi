#       reversible do |dir|
#         dir.up do
#           execute <<-SQL
#             ALTER TABLE distributors
#               ADD CONSTRAINT zipchk
#                 CHECK (char_length(zipcode) = 5) NO INHERIT;
#           SQL
#         end
#
#         dir.down do
#           execute <<-SQL
#             ALTER TABLE distributors
