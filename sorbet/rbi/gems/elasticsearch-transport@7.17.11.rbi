  # @param value the value to set the attribute timeout to.
  #
  # source://elasticsearch-transport-7.17.11/lib/elasticsearch/transport/transport/sniffer.rb:28
  def timeout=(_arg0); end

  # Returns the value of attribute transport.
  #
  # source://elasticsearch-transport-7.17.11/lib/elasticsearch/transport/transport/sniffer.rb:27
  def transport; end

  private

  # source://elasticsearch-transport-7.17.11/lib/elasticsearch/transport/transport/sniffer.rb:89
  def parse_address_port(publish_address); end

  # source://elasticsearch-transport-7.17.11/lib/elasticsearch/transport/transport/sniffer.rb:79
  def parse_publish_address(publish_address); end

  # source://elasticsearch-transport-7.17.11/lib/elasticsearch/transport/transport/sniffer.rb:72
